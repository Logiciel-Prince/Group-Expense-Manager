{
  "expo": {
      "name": "Group Expense Manager",
      "slug": "group-expense-manager",
      "version": "1.0.0",
      "orientation": "portrait",
      "icon": "./assets/icon.png",
      "userInterfaceStyle": "automatic",
      "splash": {
          "image": "./assets/splash-icon.png",
